{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-debugger": 2,
    "no-console": 0,
    "new-cap": 1,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 2,
    "eol-last": 1,
    "quotes": [2, "single"]
  }
}
